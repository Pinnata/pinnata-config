{
  "name": "@pinnata/pinnata-config",
  "version": "0.3.2",
  "description": "Configuration for the Pinnata protocol",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Pinnata/pinnata-config.git"
  },
  "keywords": [
    "celo",
    "blockchain",
    "pinnata"
  ],
  "author": "Kyle Scott <kyle@dahlia.finance>",
  "license": "AGPL-3.0",
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@saberhq/eslint-config": "^1.12.61",
    "@types/bn.js": "^5.1.0",
    "@types/eslint": "^8.4.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "17.0.24",
    "@types/prettier": "^2.6.0",
    "eslint": "^8.13.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "dependencies": {
    "@dahlia-labs/celo-contrib": "^0.3.4",
    "@dahlia-labs/celo-tokens": "^0.3.4",
    "@dahlia-labs/token-utils": "^0.3.4",
    "@ethersproject/abi": "^5.6.1",
    "@ethersproject/address": "^5.6.0",
    "@ethersproject/bignumber": "^5.6.0",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/constants": "^5.6.0",
    "@ethersproject/contracts": "^5.6.0",
    "@ethersproject/providers": "^5.6.4",
    "@saberhq/tsconfig": "^1.12.61",
    "@typechain/ethers-v5": "^10.0.0",
    "@yarnpkg/doctor": "^4.0.0-rc.2",
    "bn.js": "^5.2.0",
    "cross-env": "^7.0.3",
    "ethers": "^5.6.4",
    "jsbi": "^4.2.1",
    "lodash": "^4.17.21",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.3.1",
    "typechain": "^8.0.0",
    "typedoc": "^0.22.15"
  },
  "scripts": {
    "registry:sync": "cross-env TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' ts-node src/scripts/fetch.ts",
    "typechain": "typechain --target=ethers-v5 'src/abis/*.json' --out-dir src/generated/",
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "clean": "rm -fr dist/",
    "typecheck": "tsc --project tsconfig.cjs.json",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ts/ src/index.ts",
    "prepare": "husky install",
    "doctor:packages": "yarn doctor packages/"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{md,json,js,yml,yaml}": "prettier --write"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/Pinnata/pinnata-config/issues"
  },
  "homepage": "https://github.com/Pinnata/pinnata-config#readme"
}
