{
  "extends": "@saberhq/tsconfig/tsconfig.react.json",
  "compilerOptions": {
    "target": "ES2015",
    "module": "CommonJS",

    "strict": true,
    "strictFunctionTypes": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "exclude": ["src/generated/*"],
  "include": ["src/"]
}
